<p id="notice"><%= notice %></p>

<h1>Bicycle Reviews</h1>

<table>
  <thead>
    <tr>
      <th>Review text</th>
      <th>Bicycle</th>
      <th>User</th>
      <th>Start</th>
      <th>End</th>
      <th>Star</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bicycle_reviews.each do |bicycle_review| %>
      <tr>
        <td><%= bicycle_review.review_text %></td>
        <td><%= bicycle_review.bicycle_id %></td>
        <td><%= bicycle_review.user_id %></td>
        <td><%= bicycle_review.start %></td>
        <td><%= bicycle_review.end %></td>
        <td><%= bicycle_review.star %></td>
        <td><%= link_to 'Show', bicycle_review %></td>
        <td><%= link_to 'Edit', edit_bicycle_review_path(bicycle_review) %></td>
        <td><%= link_to 'Destroy', bicycle_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bicycle Review', new_bicycle_review_path %>
