<p id="notice"><%= notice %></p>

<h1>Bicycles</h1>

<table>
  <thead>
    <tr>
      <th>Owner</th>
      <th>Category</th>
      <th>Size</th>
      <th>Age</th>
      <th>Description</th>
      <th>Location</th>
      <th>Image</th>
      <th>Price</th>
      <th>Start</th>
      <th>End</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bicycles.each do |bicycle| %>
      <tr>
        <td><%= bicycle.owner_id %></td>
        <td><%= bicycle.category %></td>
        <td><%= bicycle.size %></td>
        <td><%= bicycle.age %></td>
        <td><%= bicycle.description %></td>
        <td><%= bicycle.location %></td>
        <td><%= bicycle.image %></td>
        <td><%= bicycle.price %></td>
        <td><%= bicycle.start %></td>
        <td><%= bicycle.end %></td>
        <td><%= link_to 'Show', [:admin, bicycle] %></td>
        <td><%= link_to 'Edit', edit_admin_bicycle_path(bicycle) %></td>
        <td><%= link_to 'Destroy', [:admin, bicycle], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bicycle', new_admin_bicycle_path %>
